@model ViewModels.EmailViewModel

<div class="card">
    <div class="card-body">
        <h5 class="card-title">
            @Model.Subject
        </h5>
        <h6 class="card-subtitle mb-2 text-muted">
            @Model.ReceivedUtcDateTime
        </h6>
        <p class="card-text">
            @Model.Body
        </p>
    </div>
</div>
